  "main": "bin/src/index.js",
  "scripts": {
    "build": "del-cli bin/ --force && babel src/ --out-dir bin/src/ && babel test/ --out-dir bin/test/",
    "lint": "eslint --plugin markdown --ext js,md .",
    "test:mocha": "mocha -r ./env/register-babel ./test/tests --recursive --exclude **/src/**",
    "test:mocha:build": "mocha -r ./env/register-tests ./bin/test/tests --recursive --exclude **/src/**",
    "test": "npm run lint && npm run test:mocha && npm run test:mocha:build",
    "watch:mocha": "mocha -r ./env/register-babel ./test/tests --recursive --exclude **/src/** --watch --watch-extensions \"js){100}|^(((?!([\\\\/](dist|tmp|node_modules|reports|\\.[^\\\\/]*)[\\\\/]|\\.(log|bak)$)).)*\"",
    "perf:mocha": "mocha -r ./env/register-babel ./test/performance",
    "coverage:mocha": "nyc npm run test:mocha",
    "coverage:mocha:build": "nyc --report-dir ./tmp/coverage/bin/ npm run test:mocha",
    "coverage": "npm run lint && npm run coverage:mocha && npm run coverage:mocha:build",
    "ci": "npm run lint && npm run coverage && ncat ./tmp/coverage/**/lcov.info --banner \"\" | coveralls"
  },
